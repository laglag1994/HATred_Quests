generator client {
  provider = "prisma-client-js"
  previewFeatures = ["createMany"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Hatred {
  id         Int       @id @default(autoincrement())
  hatName    String
  hatImg     String
  mapName    String
  mapImg     String
  mapLvlFrom Int
  mapLvlTo   Int
  info       String
  tier       String?
  monsters   Monster[]
}

model Monster {
  id          Int      @id @default(autoincrement())
  monsterName String
  hatreds     Hatred[]
}
