generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Hatred {
  id       Int       @id @default(autoincrement())
  hatName  String
  hatImg   String
  mapName  String
  mapImg   String
  mapLvl   String
  info     String
  tier     String?
  monsters Monster[]
}

model Monster {
  id          Int      @id @default(autoincrement())
  monsterName String
  hatreds     Hatred[]
}
